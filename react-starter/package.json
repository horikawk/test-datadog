{
  "name": "cts-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:tsc": "tsc --noEmit --watch",
    "dev:vite": "vite",
    "test": "npm run test:vitest",
    "test:coverage": "npm run test:vitest -- --coverage",
    "test:ci": "npm run test:vitest -- run --reporter=junit --outputFile=../junit.xml --coverage --passWithNoTests",
    "test:vitest": "vitest",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "npm run format:check",
    "format:check": "npm run format:prettier -- --check",
    "format:fix": "npm run format:prettier -- --write",
    "format:prettier": "prettier ./",
    "lint": "npm run lint:check",
    "lint:check": "npm run lint:eslint",
    "lint:fix": "npm run lint:eslint -- --fix",
    "lint:eslint": "eslint --ext .js,.cjs,.mjs,.ts,.jsx,.tsx ./src"
  },
  "dependencies": {
    "@datadog/browser-logs": "^4.30.1",
    "@hookform/resolvers": "^2.9.9",
    "@tanstack/react-query": "^4.12.0",
    "@tanstack/react-query-devtools": "^4.12.0",
    "axios": "^1.1.3",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.37.0",
    "react-icons": "^4.6.0",
    "react-router-dom": "^6.4.2",
    "recoil": "^0.7.6",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@datadog/datadog-ci": "^2.3.0",
    "@mswjs/data": "^0.10.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.24.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^20.0.1",
    "msw": "^0.47.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
